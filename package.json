{
  "name": "@barry127/bui-build-template",
  "version": "0.1.0",
  "description": "bui React Components",
  "private": true,
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "author": {
    "name": "Barry de Kleijn",
    "email": "kleijn.barry@gmail.com",
    "url": "https://github.com/barry127"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barry127/bui.git"
  },
  "bugs": {
    "url": "https://github.com/Barry127/bui/issues"
  },
  "homepage": "http://barry127.github.io/bui/",
  "keywords": [
    "components",
    "react",
    "react-component",
    "toolkit"
  ],
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@barry127/bui-feather-icons": "^0.1.0",
    "@reach/router": "^1.2.1",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^11.0.4",
    "react-docgen": "^2.21.0",
    "react-dom": "^16.5.2",
    "react-syntax-highlighter": "^10.1.2",
    "resolve": "1.8.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.11",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "node scripts/buildLib.js && rollup -c config/rollup.config.js",
    "clear": "rimraf build lib",
    "docs:gen": "node scripts/generateComponentData.js",
    "docs:gen:watch": "npm run docs:gen -- --watch",
    "docs:start": "node scripts/start.js",
    "docs:build": "node scripts/build.js",
    "predocs:deploy": "npm run docs:build",
    "docs:deploy": "gh-pages -d build",
    "start": "npm-run-all --parallel docs:start docs:gen:watch",
    "test": "node scripts/test.js",
    "prepublish": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "react-app",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": true
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "./config/jest/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "downshift": "^3.1.5",
    "lodash": "^4.17.19",
    "mousetrap": "^1.6.2",
    "prop-types": "^15.6.2",
    "react-autosize-textarea": "^5.0.0",
    "react-spring": "^7.1.3"
  }
}
